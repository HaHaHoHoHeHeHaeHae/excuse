<?xml version="1.0" encoding="utf-8" ?>

<decorators defaultdir="/WEB-INF/decorators">	
	<decorator name="main" page="decorator.jsp">
		<pattern>*/main/main_frame</pattern>		
	</decorator>
	
	<decorator name="notice" page="notice_decorator.jsp">
		<pattern>/notice/regist</pattern>
		<pattern>/notice/list</pattern>
		<pattern>/notice/list_admin</pattern>
		<pattern>/notice/detail</pattern>	
		<pattern>/notice/modify</pattern>	
	</decorator>		
		
	<decorator name="mypage" page="mypage_decorator.jsp">
		<pattern>/mypage/myclub</pattern>
		<pattern>/mypage/myinfo</pattern>	
		<pattern>/mypage/wishlist</pattern>	
		<pattern>/mypage/myjoinlist</pattern>	
		<pattern>/mypage/myreply</pattern>					
	</decorator>	
	
	<decorator name="manage" page="manage_decorator.jsp">
		<pattern>/manage/*</pattern>		
	</decorator>	
	

	
<!-- 	<decorator name="OpenWindow" page="open_decorator.jsp">
		<pattern>/*</pattern>		
	</decorator>
	 -->
	
	
	<excludes>
		<pattern>*/common/login*</pattern>
		<pattern>*/common/regist*</pattern>
		<pattern>*/common/search*</pattern>
		<pattern>*/club/detail</pattern>		
		<pattern>*/memberdetail*</pattern>
		<pattern>*/replyList*</pattern>
		<pattern>/mypage/myclubuser</pattern>	
		<!-- <pattern>/mypage/listForm</pattern> -->
		
	</excludes>
</decorators>
